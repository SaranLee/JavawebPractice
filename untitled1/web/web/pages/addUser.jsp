<%-- 设置高亮 --%>
<script>
    setHighlightAndMenuOpen("用户管理", "添加用户");
    <%--setNavHref("${pageContext.request.contextPath}");--%>

    //点击添加用户按钮
    $("#btn_addUser").click(function () {
        var username = $("input[name='username']").val();
        var password = $("input[name='password']").val();
        //ajax请求是否已经存在该用户名
        $.post("user?method=exist", {username:username}, function (respText) {
            if(respText === "true"){
                $("input[name='username']").addClass("is-invalid");
                layer.tips('该用户名已存在', '#username', {time: 2000, tips:[2, "#ca3546"], anim:6});
            }else{
                //ajax请求添加用户
                $.post("${pageContext.request.contextPath}/user?method=add", {username:username, password:password}, function(respText){
                    if(respText === "ok"){
                        layer.msg("添加成功", {time:1000, offset:"auto"}, function(){
                            window.location = "${pageContext.request.contextPath}/viewpage?pageName=addUser.jsp";
                        });
                    }
                }, "text");
            }
        }, "text");
        return false;
    });

    //用户名输入框失去焦点，判断用户名是否重复
    $("#username").blur(function () {
        var username = $("input[name='username']").val();
        //ajax请求是否已经存在该用户名
        $.post("user?method=exist", {username:username}, function (respText) {
            if(respText === "true"){
                $("input[name='username']").addClass("is-invalid");
                layer.tips('该用户名已存在', '#username', {time: 2000, tips:[2, "#ca3546"]});
            }else{
                $("#username").removeClass("is-invalid");
            }
        }, "text");
        return false;
    });

    //点击用户头像，打开layer修改用户头像
    $("#login_user_avatar").click(function () {
        layer.open({
            type: 2,
            title: false,
            shadeClose: true,
            shade: 0.6,
            area: ['400px', '360px'],
            offset: "100px",
            closeBtn: 0,
            content: 'pages/modifyAvatar.jsp' //iframe的url
        });
    })
</script>

